{
  "version": 2,
  "routes": [
    { "src": "^/assets/(.*)$", "dest": "/assets/$1" },
    { "src": "^/dist/assets/(.*)$", "dest": "/dist/assets/$1" },
    { "src": "^/(.*\\.(js|mjs|css|map|png|jpg|jpeg|webp|svg|ico|json))$", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
